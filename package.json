{
  "name": "accent-cli",
  "version": "0.3.0",
  "author": "Simon PrÃ©vost",
  "description": "Accent CLI",
  "bin": {
    "accent": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "~> 1.4",
    "@oclif/config": "1.3.62",
    "@oclif/plugin-help": "1.2.2",
    "@oclif/plugin-not-found": "~> 1.0",
    "@types/decamelize": "1.2.0",
    "@types/form-data": "2.2.1",
    "@types/fs-extra": "5.0.1",
    "@types/glob": "5.0.35",
    "@types/node-fetch": "1.6.7",
    "chalk": "2.3.2",
    "cli-ux": "3.3.26",
    "decamelize": "2.0.0",
    "form-data": "2.3.2",
    "glob": "7.1.2",
    "node-fetch": "2.6.1",
    "tslib": "1.9.0",
    "tslint-config-prettier": "1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.4.2",
    "@oclif/test": "1.0.1",
    "@oclif/tslint": "1.0.2",
    "@types/chai": "4.1.2",
    "@types/mocha": "5.0.0",
    "@types/node": "9.6.0",
    "chai": "4.1.2",
    "globby": "8.0.1",
    "mocha": "5.0.5",
    "prettier": "1.11.1",
    "ts-node": "5",
    "tslint": "5.9.1",
    "typescript": "2.7.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/bin",
    "/lib"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "accent",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ]
  },
  "scripts": {
    "build": "rm -rf lib && tsc",
    "clean": "rm -f .oclif.manifest.json",
    "lint": "tslint --project tsconfig.json 'src/**/*.{ts,tsx}'",
    "fix": "tslint --project tsconfig.json --fix 'src/**/*.{ts,tsx}'",
    "prepublishOnly": "npm run build && oclif-dev readme && oclif-dev manifest",
    "prettier": "prettier --single-quote --no-bracket-spacing --no-semi --write './src/**/*.{js,json,ts,tsx,gql}'",
    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
